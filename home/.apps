# Path to the bash it configuration
export BASH_IT="$HOME/ownCloud/settings/.bash_it"

# Lock and Load a custom theme file
export BASH_IT_THEME="envy"

# Load Bash It
source $BASH_IT/bash_it.sh

# Automatically check for identity updates before running a git command
if which karn > /dev/null; then eval "$(karn init)"; fi

# Load project directories @todo
export PROJECT_PATHS=~/Code/Infra:~/Code/JavaScript:~/Code/Kitchenplan:~/Code/PHP:~/Code/Projects:~/Code/Wordpress:~/Code/Wordpress/plugins:~/Code/Wordpress/sites:~/Code/Wordpress/themes
